<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <name>Maven HTMLCompressor Plugin</name>
  <description>Maven HTMLCompressor Plugin allows to compress html/xml by adding a few lines to the pom file.</description>
  <groupId>com.tunyk.mvn.plugins.htmlcompressor</groupId>
  <artifactId>htmlcompressor-maven-plugin</artifactId>
  <version>1.3</version>
  <goalPrefix>htmlcompressor</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>xml</goal>
      <description>Compress XML files</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <implementation>com.tunyk.mvn.plugins.htmlcompressor.XmlCompressorMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>enabled</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>if false all compression is off (default is true)</description>
        </parameter>
        <parameter>
          <name>encoding</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>Charset encoding for files to read and create</description>
        </parameter>
        <parameter>
          <name>fileExt</name>
          <type>java.lang.String[]</type>
          <required>false</required>
          <editable>true</editable>
          <description>file types to be processed</description>
        </parameter>
        <parameter>
          <name>removeComments</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>if false keeps XML comments (default is true)</description>
        </parameter>
        <parameter>
          <name>removeIntertagSpaces</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>removes iter-tag whitespace characters  (default is true)</description>
        </parameter>
        <parameter>
          <name>srcFolder</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>source folder where xml files are located.</description>
        </parameter>
        <parameter>
          <name>targetFolder</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>target folder where compressed xml files will be placed.</description>
        </parameter>
      </parameters>
      <configuration>
        <enabled implementation="java.lang.Boolean" default-value="true">${htmlcompressor.enabled}</enabled>
        <encoding implementation="java.lang.String" default-value="utf-8">${htmlcompressor.encoding}</encoding>
        <fileExt implementation="java.lang.String[]">${htmlcompressor.fileExt}</fileExt>
        <removeComments implementation="java.lang.Boolean" default-value="true">${htmlcompressor.removeComments}</removeComments>
        <removeIntertagSpaces implementation="java.lang.Boolean" default-value="true">${htmlcompressor.removeIntertagSpaces}</removeIntertagSpaces>
        <srcFolder implementation="java.lang.String" default-value="${basedir}/src/main/resources/xml">${htmlcompressor.srcFolder}</srcFolder>
        <targetFolder implementation="java.lang.String" default-value="${project.build.directory}/htmlcompressor/xml">${htmlcompressor.targetFolder}</targetFolder>
      </configuration>
    </mojo>
    <mojo>
      <goal>html</goal>
      <description>Compress HTML files</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <implementation>com.tunyk.mvn.plugins.htmlcompressor.HtmlCompressorMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>closureCustomExternsOnly</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Disable default built-in closure externs.</description>
        </parameter>
        <parameter>
          <name>closureExterns</name>
          <type>java.lang.String[]</type>
          <required>false</required>
          <editable>true</editable>
          <description>Sets custom closure externs file list.</description>
        </parameter>
        <parameter>
          <name>closureOptLevel</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>closureOptLevel = &quot;simple&quot;, &quot;advanced&quot; or &quot;whitespace&quot;</description>
        </parameter>
        <parameter>
          <name>compressCss</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>compress inline css</description>
        </parameter>
        <parameter>
          <name>compressJavaScript</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>compress inline javascript</description>
        </parameter>
        <parameter>
          <name>enabled</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>if false all compression is off (default is true)</description>
        </parameter>
        <parameter>
          <name>encoding</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>Charset encoding for files to read and create</description>
        </parameter>
        <parameter>
          <name>fileExt</name>
          <type>java.lang.String[]</type>
          <required>false</required>
          <editable>true</editable>
          <description>file types to be processed</description>
        </parameter>
        <parameter>
          <name>generateStatistics</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>HTML compression statistics</description>
        </parameter>
        <parameter>
          <name>htmlCompressionStatistics</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>file where statistics of html compression is stored</description>
        </parameter>
        <parameter>
          <name>javascriptHtmlSprite</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Create javascript file which includes all compressed html files as json object. If set to true then javascriptHtmlSpriteIntegrationFile param is required, otherwise it will throw exception.</description>
        </parameter>
        <parameter>
          <name>javascriptHtmlSpriteIntegrationFile</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>JavaScript sprite integration file (first occurrence of &quot;%s&quot; will be substituted by json with all compressed html strings)</description>
        </parameter>
        <parameter>
          <name>javascriptHtmlSpriteTargetFile</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The target JavaScript sprite file with compressed html files as json object.</description>
        </parameter>
        <parameter>
          <name>jsCompressor</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>javascript compression: &quot;yui&quot; or &quot;closure&quot;</description>
        </parameter>
        <parameter>
          <name>predefinedPreservePatterns</name>
          <type>java.lang.String[]</type>
          <required>false</required>
          <editable>true</editable>
          <description>predefined patterns for most often used custom preservation rules: PHP_TAG_PATTERN and SERVER_SCRIPT_TAG_PATTERN.</description>
        </parameter>
        <parameter>
          <name>preservePatternFiles</name>
          <type>java.io.File[]</type>
          <required>false</required>
          <editable>true</editable>
          <description>list of files containing preserve patterns</description>
        </parameter>
        <parameter>
          <name>preservePatterns</name>
          <type>java.lang.String[]</type>
          <required>false</required>
          <editable>true</editable>
          <description>preserve patterns</description>
        </parameter>
        <parameter>
          <name>removeComments</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>if false keeps HTML comments (default is true)</description>
        </parameter>
        <parameter>
          <name>removeFormAttributes</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>remove optional attributes from form tags</description>
        </parameter>
        <parameter>
          <name>removeHttpProtocol</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>replace &quot;http://&quot; with &quot;//&quot; inside tag attributes</description>
        </parameter>
        <parameter>
          <name>removeHttpsProtocol</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>replace &quot;https://&quot; with &quot;//&quot; inside tag attributes</description>
        </parameter>
        <parameter>
          <name>removeInputAttributes</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>remove optional attributes from input tags</description>
        </parameter>
        <parameter>
          <name>removeIntertagSpaces</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>removes iter-tag whitespace characters</description>
        </parameter>
        <parameter>
          <name>removeJavaScriptProtocol</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>remove &quot;javascript:&quot; from inline event handlers</description>
        </parameter>
        <parameter>
          <name>removeLinkAttributes</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>remove optional attributes from link tags</description>
        </parameter>
        <parameter>
          <name>removeMultiSpaces</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>if false keeps multiple whitespace characters (default is true)</description>
        </parameter>
        <parameter>
          <name>removeQuotes</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>removes unnecessary tag attribute quotes</description>
        </parameter>
        <parameter>
          <name>removeScriptAttributes</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>remove optional attributes from script tags</description>
        </parameter>
        <parameter>
          <name>removeStyleAttributes</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>remove optional attributes from style tags</description>
        </parameter>
        <parameter>
          <name>simpleBooleanAttributes</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>remove values from boolean tag attributes</description>
        </parameter>
        <parameter>
          <name>simpleDoctype</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>simplify existing doctype</description>
        </parameter>
        <parameter>
          <name>srcFolder</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>source folder where html files are located.</description>
        </parameter>
        <parameter>
          <name>targetFolder</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>target folder where compressed html files will be placed.</description>
        </parameter>
        <parameter>
          <name>yuiCssLineBreak</name>
          <type>java.lang.Integer</type>
          <required>false</required>
          <editable>true</editable>
          <description>--line-break param for Yahoo YUI Compressor</description>
        </parameter>
        <parameter>
          <name>yuiJsDisableOptimizations</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>--disable-optimizations param for Yahoo YUI Compressor</description>
        </parameter>
        <parameter>
          <name>yuiJsLineBreak</name>
          <type>java.lang.Integer</type>
          <required>false</required>
          <editable>true</editable>
          <description>--line-break param for Yahoo YUI Compressor</description>
        </parameter>
        <parameter>
          <name>yuiJsNoMunge</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>--nomunge param for Yahoo YUI Compressor</description>
        </parameter>
        <parameter>
          <name>yuiJsPreserveAllSemiColons</name>
          <type>java.lang.Boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>--preserve-semi param for Yahoo YUI Compressor</description>
        </parameter>
      </parameters>
      <configuration>
        <closureCustomExternsOnly implementation="java.lang.Boolean" default-value="false">${htmlcompressor.closureCustomExternsOnly}</closureCustomExternsOnly>
        <closureExterns implementation="java.lang.String[]">${htmlcompressor.closureExterns}</closureExterns>
        <closureOptLevel implementation="java.lang.String" default-value="simple">${htmlcompressor.closureOptLevel}</closureOptLevel>
        <compressCss implementation="java.lang.Boolean" default-value="false">${htmlcompressor.compressCss}</compressCss>
        <compressJavaScript implementation="java.lang.Boolean" default-value="false">${htmlcompressor.compressJavaScript}</compressJavaScript>
        <enabled implementation="java.lang.Boolean" default-value="true">${htmlcompressor.enabled}</enabled>
        <encoding implementation="java.lang.String" default-value="utf-8">${htmlcompressor.encoding}</encoding>
        <fileExt implementation="java.lang.String[]">${htmlcompressor.fileExt}</fileExt>
        <generateStatistics implementation="java.lang.Boolean" default-value="true">${htmlcompressor.generateStatistics}</generateStatistics>
        <htmlCompressionStatistics implementation="java.lang.String" default-value="${project.build.directory}/htmlcompressor/html-compression-statistics.txt">${htmlcompressor.htmlCompressionStatistics}</htmlCompressionStatistics>
        <javascriptHtmlSprite implementation="java.lang.Boolean" default-value="true">${htmlcompressor.javascriptHtmlSprite}</javascriptHtmlSprite>
        <javascriptHtmlSpriteIntegrationFile implementation="java.lang.String" default-value="${basedir}/src/main/resources/html/integration.js">${htmlcompressor.javascriptHtmlSpriteIntegrationFile}</javascriptHtmlSpriteIntegrationFile>
        <javascriptHtmlSpriteTargetFile implementation="java.lang.String" default-value="${project.build.directory}/htmlcompressor/html/integration.js">${htmlcompressor.javascriptHtmlSpriteTargetFile}</javascriptHtmlSpriteTargetFile>
        <jsCompressor implementation="java.lang.String" default-value="yui">${htmlcompressor.jsCompressor}</jsCompressor>
        <predefinedPreservePatterns implementation="java.lang.String[]">${htmlcompressor.predefinedPreservePatterns}</predefinedPreservePatterns>
        <preservePatternFiles implementation="java.io.File[]">${htmlcompressor.preservePatternFiles}</preservePatternFiles>
        <preservePatterns implementation="java.lang.String[]">${htmlcompressor.preservePatterns}</preservePatterns>
        <removeComments implementation="java.lang.Boolean" default-value="true">${htmlcompressor.removeComments}</removeComments>
        <removeFormAttributes implementation="java.lang.Boolean" default-value="false">${htmlcompressor.removeFormAttributes}</removeFormAttributes>
        <removeHttpProtocol implementation="java.lang.Boolean" default-value="false">${htmlcompressor.removeHttpProtocol}</removeHttpProtocol>
        <removeHttpsProtocol implementation="java.lang.Boolean" default-value="false">${htmlcompressor.removeHttpsProtocol}</removeHttpsProtocol>
        <removeInputAttributes implementation="java.lang.Boolean" default-value="false">${htmlcompressor.removeInputAttributes}</removeInputAttributes>
        <removeIntertagSpaces implementation="java.lang.Boolean" default-value="false">${htmlcompressor.removeIntertagSpaces}</removeIntertagSpaces>
        <removeJavaScriptProtocol implementation="java.lang.Boolean" default-value="false">${htmlcompressor.removeJavaScriptProtocol}</removeJavaScriptProtocol>
        <removeLinkAttributes implementation="java.lang.Boolean" default-value="false">${htmlcompressor.removeLinkAttributes}</removeLinkAttributes>
        <removeMultiSpaces implementation="java.lang.Boolean" default-value="true">${htmlcompressor.removeMultiSpaces}</removeMultiSpaces>
        <removeQuotes implementation="java.lang.Boolean" default-value="false">${htmlcompressor.removeQuotes}</removeQuotes>
        <removeScriptAttributes implementation="java.lang.Boolean" default-value="false">${htmlcompressor.removeScriptAttributes}</removeScriptAttributes>
        <removeStyleAttributes implementation="java.lang.Boolean" default-value="false">${htmlcompressor.removeStyleAttributes}</removeStyleAttributes>
        <simpleBooleanAttributes implementation="java.lang.Boolean" default-value="false">${htmlcompressor.simpleBooleanAttributes}</simpleBooleanAttributes>
        <simpleDoctype implementation="java.lang.Boolean" default-value="false">${htmlcompressor.simpleDoctype}</simpleDoctype>
        <srcFolder implementation="java.lang.String" default-value="${basedir}/src/main/resources/html">${htmlcompressor.srcFolder}</srcFolder>
        <targetFolder implementation="java.lang.String" default-value="${project.build.directory}/htmlcompressor/html">${htmlcompressor.targetFolder}</targetFolder>
        <yuiCssLineBreak implementation="java.lang.Integer" default-value="-1">${htmlcompressor.yuiCssLineBreak}</yuiCssLineBreak>
        <yuiJsDisableOptimizations implementation="java.lang.Boolean" default-value="false">${htmlcompressor.yuiJsDisableOptimizations}</yuiJsDisableOptimizations>
        <yuiJsLineBreak implementation="java.lang.Integer" default-value="-1">${htmlcompressor.yuiJsLineBreak}</yuiJsLineBreak>
        <yuiJsNoMunge implementation="java.lang.Boolean" default-value="false">${htmlcompressor.yuiJsNoMunge}</yuiJsNoMunge>
        <yuiJsPreserveAllSemiColons implementation="java.lang.Boolean" default-value="false">${htmlcompressor.yuiJsPreserveAllSemiColons}</yuiJsPreserveAllSemiColons>
      </configuration>
    </mojo>
  </mojos>
